<div class="my-card">
            <div class="card-header">
                <img src="{{image}}" />
            </div>
            <div class="card-body">
                <span class="tag">Top Speed: {{topSpeed}}</span>
                <h4>
                    {{model}}
                </h4>
                <div class="user">
                    <div class="user-info">
                        <h5>Manufacturer: {{manufacturer}}</h5>
                        <small>Engine: {{engine}}</small>
                        <a href="/cars/{{id}}/details" class="details_btn">Details</a>
                    </div>
                </div>
            </div>
        </div>